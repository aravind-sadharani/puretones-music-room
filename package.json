{
  "name": "pure-tones-music-room",
  "main": "electronApp.js",
  "build": {
    "icon": "src/images/puretones.png",
    "productName": "PureTones Electron App",
    "files": [
      "public/**/*",
      "electronApp.js"
    ],
    "win": {},
    "linux": {},
    "mac": {}
  },
  "version": "1.0.0",
  "private": true,
  "description": "PureTones Music Room",
